{
  "$schema": "http://json.schemastore.org/template",
  "identity": "Finebits.Dotnet.Templates.Console",
  "author": "Finebits",

  "classifications": ["Console"],

  "name": "Finebits Console App",
  "shortName": "finebits-console-app",
  "defaultName": "Console.App",
  "sourceName": "Template.Console",

  "tags": {
    "language": "C#",
    "type": "project"
  },

  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0",
          "description": "Target net6.0"
        },
        {
          "choice": "net7.0",
          "description": "Target net7.0"
        },
        {
          "choice": "net8.0",
          "description": "Target net8.0"
        }
      ],
      "replaces": "net7.0",
      "defaultValue": "net7.0"
    },
  }
}
